{
  "name": "nuber-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-boost": "^0.4.0",
    "graphql": "^14.3.0",
    "prop-types": "^15.7.2",
    "react": "^16.4.2",
    "react-apollo": "^2.5.6",
    "react-dom": "^16.4.2",
    "react-facebook-login": "^4.1.1",
    "react-helmet": "^5.2.1",
    "react-router-dom": "^5.0.0",
    "react-scripts-ts": "2.17.0",
    "react-sidebar": "^3.0.2",
    "react-toastify": "^5.1.1",
    "styled-components": "^4.2.0",
    "styled-reset": "^2.0.12"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "precodegen": "apollo schema:download --endpoint=http://localhost:4000/graphql",
    "codegen": "apollo codegen:generate src/types/api.d.ts --queries='src/**/!(*.local).ts' --addTypename --localSchemaFile=schema.json --target typescript --outputFlat",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^10.14.7",
    "@types/prop-types": "^15.5.4",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.0.7",
    "@types/react-sidebar": "^3.0.0",
    "axios": "^0.19.0",
    "gh-pages": "^2.0.1",
    "typescript": "^3.0.1"
  }
}
